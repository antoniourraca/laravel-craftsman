{
    "name": "laravel-craftsman",
    "description": "Laravel Craftsman",
    "keywords": [
        "framework",
        "laravel",
        "laravel zero",
        "console",
        "cli"
    ],
    "homepage": "https://laravel-craftsman.com",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/mikeerickson/laravel-craftsman/issues",
        "source": "https://github.com/mikeerickson/laravel-craftsman"
    },
    "authors": [
        {
            "name": "Mike Erickson",
            "email": "codedungeon@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "laravel-zero/framework": "5.8.*"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.26.1",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "scripts": {
        "post-create-project-cmd": [
            "@php application app:rename"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "laravel-craftsman"
    ]
}
